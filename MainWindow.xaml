        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,91,10,503" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Flash A/B ROMs" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,136,10,458" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Flash TWRP A" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,181,10,413" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Flash TWRP A/B" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,226,10,368" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Boot TWRP" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,271,10,323" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Unlock Bootloader" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
