        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,451,10,143" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Go to XDA" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,496,10,98" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="View Changelog" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" Height="40" Margin="10,541,10,53" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Uninstall" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Button Content="Button" Margin="0,91,0,503" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_1"/>
        <Button Content="Button" Margin="0,136,0,458" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_2"/>
        <Button Content="Button" Margin="0,181,0,413" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_3"/>
        <Button Content="Button" Margin="0,226,0,368" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_4"/>
        <Button Content="Button" Margin="0,271,0,323" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_5"/>
        <Button Content="Button" Margin="0,316,0,278" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_6"/>
