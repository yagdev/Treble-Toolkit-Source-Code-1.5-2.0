        <Button Content="Button" Margin="0,541,0,93" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_11"/>
        <Button Content="Button" Margin="0,46,0,588" VerticalAlignment="Center" Height="40" Opacity="0" HorizontalContentAlignment="Stretch" Grid.IsSharedSizeScope="True" Click="Button_Click"/>
