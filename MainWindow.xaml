        <Button Content="Button" Margin="0,541,0,53" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_11"/>
        <Button Content="Button" Margin="0,46,0,548" VerticalAlignment="Center" Height="40" Opacity="0" HorizontalContentAlignment="Stretch" Grid.IsSharedSizeScope="True" Click="Button_Click"/>
        <Border BorderThickness="1" Height="40" Margin="10,586,10,8" VerticalAlignment="Center" Background="#FF4D4D4D">
            <Label Content="Compatible Devices" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontFamily="Segoe UI Semibold" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <Button Content="Button" Margin="0,586,0,8" VerticalAlignment="Center" Height="40" Opacity="0" Click="Button_Click_12"/>
    </Grid>
